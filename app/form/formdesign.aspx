	ViewBuilder.Append("            </td>\r\n");
	ViewBuilder.Append("            <td style=\"background:#ffffff\" valign=\"top\" align=\"center\">\r\n");
	ViewBuilder.Append("                <table width=\"120\" border=\"0\" cellspacing=\"1\" class=\"small\" bgcolor=\"#000000\" cellpadding=\"3\" align=\"center\">\r\n");
	ViewBuilder.Append("                    <tr class=\"TableHeader\" style=\"height:30px;\">\r\n");
	ViewBuilder.Append("                        <td align=\"center\">\r\n");
	ViewBuilder.Append("                            表单控件\r\n");
	ViewBuilder.Append("                        </td>\r\n");
	ViewBuilder.Append("                    </tr>\r\n");
	ViewBuilder.Append("                    <tr>\r\n");
