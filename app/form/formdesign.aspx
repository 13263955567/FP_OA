	ViewBuilder.Append("</head>\r\n");
	ViewBuilder.Append("<body class=\"bodycolor\">\r\n");
	ViewBuilder.Append("    <table width=\"100%\" height=\"100%\" class=\"big\" bgcolor=\"#000000\" border=\"0\" cellpadding=\"3\" cellspacing=\"1\" align=\"center\">\r\n");
	ViewBuilder.Append("        <tr bgcolor=\"#DDDDDD\">\r\n");
