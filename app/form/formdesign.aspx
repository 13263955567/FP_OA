	ViewBuilder.Append("<style type=\"text/css\">\r\n");
	ViewBuilder.Append("    .TableHeader { COLOR:#2E2E5A; FONT-WEIGHT: bold; FONT-SIZE: 9pt; background:url(statics/images/list_hd_bg.png); line-height:21px;}\r\n");
